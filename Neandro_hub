local dragFrame = Instance.new("Frame", ScreenGui)
dragFrame.Size = UDim2.new(0, 300, 0, 400)
dragFrame.Position = UDim2.new(0.1, 0, 0.1, 0)
dragFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- preto
